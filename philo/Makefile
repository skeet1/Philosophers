endef
export	HEADER_G

all : $(NAME)

$(NAME): print_header $(OBJ) $(HEADER)
	$(CC) $(FLAGS) $(OBJ) -o $(NAME)
	@echo "\t\t\t\033[0;35m \033[1m Philo is created successfully\033[0m"

print_header:
	@echo "\033[0;32m$$HEADER_G" 

%.o : %.c
	$(CC) $(FLAGS) -c $<

clean :
	rm $(OBJ)
	@echo "\033[1;33m>> all objects files are deleted.\033[0m"

fclean : clean
	rm -rf $(NAME)
	@echo "\033[0;31m>> $(NAME) is deleted.\033[0m"

re : fclean all
